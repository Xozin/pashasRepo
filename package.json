{
  "name": "pashasrepo",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "prepare": "git config core.hooksPath .git-hooks || echo 'Not in a git repo'",
    "lint": "oxlint .",
    "preview": "vite preview",
    "dev": "cross-env VITE_DEV_SERVER_URL=http://localhost:5173 concurrently \"vite\" \"electron .\"",
    "build": "vite build && electron-builder",
    "start": "electron .",
    "format": "prettier --write \"**/*.{ts,tsx,json}\" && oxlint --fix"
  },
  "lint-staged": {
    "*.{ts,tsx,json}": [
      "prettier --write \"**/*.{ts,tsx,json}\"",
      "oxlint --fix",
      "git add"
    ]
  },
  "main": "electron/main.js",
  "build": {
    "appId": "com.example.app",
    "files": ["dist/**/*", "electron/**/*"],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.1.0",
    "antd": "^5.25.3",
    "concurrently": "^9.1.2",
    "convert-excel-to-json": "^1.7.0",
    "cross-env": "^7.0.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-window": "^1.8.11",
    "sass": "^1.87.0",
    "vite-plugin-oxlint": "^1.3.3"
  },
  "devDependencies": {
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "electron": "^36.2.0",
    "electron-builder": "^26.0.12",
    "globals": "^16.0.0",
    "lint-staged": "^16.1.0",
    "oxlint": "^0.17.0",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  }
}
